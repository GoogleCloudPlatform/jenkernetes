{
	"metadata": {
		"name": "jenkins"
	},
	"kind": "Service",
	"apiVersion": "v1",
	"spec": {
		"createExternalLoadBalancer": true,
		"ports": [
			{
				"name": "webui",
				"port": 8080,
				"targetPort": "webui"
			},
			{
				"name": "slavelistener",
				"port": 50000,
				"targetPort": "slavelistener"
			}
		],
		"selector": {
			"role": "master"
		}
	}
}
